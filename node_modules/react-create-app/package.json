{
    "name": "react-create-app",
    "version": "2.0.6",
    "license": "MIT",
    "author": "Damiano Barbati <damiano.barbati@gmail.com> (http://github.com/damianobarbati)",
    "main": "src/index.js",
    "module": "src/index.js",
    "jsnext:main": "src/index.js",
    "scripts": {
        "jslint": "eslint **/*.js --ignore-path .gitignore --ignore-pattern **/*.min.js ."
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "env": {
            "es6": true,
            "browser": true,
            "node": true,
            "jest": true
        },
        "parserOptions": {
            "ecmaVersion": 2017,
            "sourceType": "module",
            "allowImportExportEverywhere": false,
            "impliedStrict": true,
            "ecmaFeatures": {
                "jsx": true,
                "impliedStrict": true,
                "globalReturn": false,
                "experimentalObjectRestSpread": true
            }
        },
        "plugins": [
            "import",
            "react",
            "jest"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended",
            "plugin:jest/recommended"
        ],
        "rules": {
            "no-console": "off"
        }
    },
    "babel": {
        "presets": [
            [
                "env",
                {
                    "modules": false,
                    "useBuiltIns": "usage",
                    "debug": true
                }
            ],
            "stage-0",
            "react"
        ],
        "plugins": [
            "transform-decorators-legacy",
            "transform-class-properties"
        ]
    },
    "dependencies": {
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.2",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-plugin-transform-decorators-legacy": "1.3.4",
        "babel-preset-env": "next",
        "babel-preset-react": "6.24.1",
        "babel-preset-stage-0": "6.24.1",
        "eslint": "4.5.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-import": "2.7.0",
        "eslint-plugin-jest": "20.0.3",
        "eslint-plugin-react": "7.3.0",
        "prop-types": "15.5.10",
        "react": "15.6.1",
        "react-redux": "5.0.6",
        "redux": "3.7.2",
        "redux-persist": "4.9.1",
        "redux-persist-crosstab": "3.6.0"
    }
}
